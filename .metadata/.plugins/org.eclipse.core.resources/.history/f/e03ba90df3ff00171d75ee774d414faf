package JavaTest_9;

abstract class Employee
{
	private String name;
	private String id;
	private double pay;
	Employee(String name,String id,double pay)
	{
		this.name = name;
		this.id = id;
		this.pay = pay;
	}

	public abstract void work();
	
}

public class AbstractTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
