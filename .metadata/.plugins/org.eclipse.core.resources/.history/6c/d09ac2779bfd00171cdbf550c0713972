package JavaTest_4;

public class ForForTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int z = 5;
		for (int x = 1; x<=5 ;x++ )//1-5 1-4 1-3//1-5 2-5 3-5
		{
			for (int y=1; y<=z ; y++ )
			{
				System.out.print("*");
			}
			System.out.println();
			z--;
		}
		
	}

}
