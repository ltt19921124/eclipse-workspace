package JavaTest_5;

public class ArrayTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static void printArray(int[] arr)
	{
		System.out.print("[");
		for(int x=0; x<arr.length; x++)
		{
			if(x!=arr.length-1)
				System.out.print(arr[x]+", ");
			else
				System.out.println(arr[x]+"]");
		}
	}
	
}
